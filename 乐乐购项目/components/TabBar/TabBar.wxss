/* components/TabBar/TabBar.wxss */
.tabbar {
  background-color: #fff;
}
.tabbar .tabbar-title {
  display: flex;
}
.tabbar .tabbar-title .tabbar-item {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  padding: 15rpx 0;
}
.tabbar .active {
  color: var(--themeColor);
  border-bottom: 5rpx solid;
}
